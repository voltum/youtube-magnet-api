    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.2.6",
    "@nestjs/websockets": "^8.2.6",
    "@types/json2csv": "^5.0.3",
    "bull": "^4.2.0",
    "cld": "^2.7.1",
    "csvtojson": "^2.0.10",
    "fast-csv": "^4.3.6",
    "googleapis": "^92.0.0",
    "json2csv": "^5.0.6",
